  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Start server",
      "program": "${workspaceFolder}\\src\\server.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start server (w/o internals)",
      "program": "${workspaceFolder}\\src\\server.js",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start server (w/o internals/modules)",
      "program": "${workspaceFolder}\\src\\server.js",
      "skipFiles": ["node_modules/**", "<node_internals>/**"]
    }
  ]
}
